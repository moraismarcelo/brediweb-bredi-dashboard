{
    "name": "brediweb/bredi-dashboard",
    "description": "Dashboard base em Laravel",
    "keywords": [
        "laravel",
        "laravel-dashboard",
        "dashboard",
        "base-dashboard"
    ],
    "authors": [{
        "name": "Admin Bredi",
        "email": "contato@bredi.com.br"
    }],
    "extra": {
        "laravel": {
            "providers": ["Brediweb\\BrediDashboard\\Providers\\BrediDashboardServiceProvider"],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Brediweb\\BrediDashboard\\": "src/"
        },
        "files": ["src/Http/Helper/Helper.php"]
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-cors": "^3.0.0",
        "igaster/laravel-theme": "^2.0",
        "laravelcollective/html": "6.3.0",
        "brediweb/imagemupload": "7.x-dev",
        "arcanedev/log-viewer": "7.0.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/moraismarcelo/brediweb-imagemupload"
        }
    ]
}
